<%  text = @object_hash[:content]
   rows = text.split("\n")
   %>
<a class="table-button"><img  src="/images/play_button_small.png" alt="run tests"/></a>
<a class="table-button"><img src="/images/edit_small.png" alt="edit table"/></a>
<a class="table_button" onclick="javascript: deleteElement('<%= @object_hash[:name] %>')">X</a>
<p>Fixture Class: <strong>mock_fixture</strong></p>
<table cellpadding="0" cellspacing="0">
    <thead>
        <tr>
            <% puts rows[0]; rows[0].split("|").each do |cell|%>
            <% if cell != "" then %>
                <td><%= cell %></td>
            <% end %>
          <% end %>
        </tr>
    </thead>
    <% 1.upto(rows.size - 1) do |i| %>
       <tr>
         <% cells = rows[i].strip.split("|")
            cells.each do |cell|%>
            <% if cell != "" then %>
                <td><%= cell %></td>
            <% end %>
          <% end %>
        </tr>
    <% end %>
 </table>